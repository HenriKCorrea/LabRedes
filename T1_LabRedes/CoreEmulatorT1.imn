node n2 {
    type router
    model router
    network-config {
	hostname gateway
	!
	interface eth0
	 ip address 10.0.0.1/24
	 ipv6 address 2001:0::1/64
	!
    }
    canvas c1
    iconcoords {273.0 206.0}
    labelcoords {273.0 238.0}
    interface-peer {eth0 n1}
}

