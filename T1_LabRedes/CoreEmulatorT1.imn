node n1 {
    type hub
    network-config {
	hostname hub
	!
    }
    canvas c1
    iconcoords {347.0 307.0}
    labelcoords {347.0 331.0}
    interface-peer {e0 n3}
    interface-peer {e1 n2}
    interface-peer {e2 n4}
    interface-peer {e3 n5}
}

link l1 {
    nodes {n3 n1}
    bandwidth 0
}

link l2 {
    nodes {n2 n1}
    bandwidth 0
}

link l3 {
    nodes {n1 n4}
    bandwidth 0
}

link l4 {
    nodes {n1 n5}
    bandwidth 0
}

canvas c1 {
    name {Canvas1}
}

option global {
    interface_names no
    ip_addresses yes
    ipv6_addresses yes
    node_labels yes
    link_labels yes
    show_api no
    background_images no
    annotations yes
    grid yes
    traffic_start 0
}

option session {
}

